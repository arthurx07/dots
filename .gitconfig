[credential "https://github.com"]
  helper = 
  helper = !/usr/bin/gh auth git-credential
[user]
  name = kumao
  email = arturleivarx@gmail.com
[alias]
  ad = add .
  br = branch
  ci = commit
  co = checkout
  cl = clone
  lo = log --oneline -n 15
  # lo = log
  st = status
  up = pull --rebase
  re = restore
  last = log -1 HEAD
  df = diff
[push]
  default = current
[core]
  editor = nvim -c 'put_' -c 'startinsert'
  excludesfile = ~/.config/.global-gitignore
[init]
  defaultBranch = main
[log]
  date = relative
[format]
  pretty = format:%C(auto,yellow)%h%C(auto,magenta)% G? %C(auto,blue)%>(12,trunc)%ad %C(auto,green)%<(7,trunc)%aN%C(auto,reset)%s%C(auto,red)% gD% D

! settings for dwm, do not change [dpi and cursor are necessary]
Xcursor.theme: Bibata-Modern-Ice
Xft.dpi:       96
! font settings [taken from a post on linuxquestions.org 2022]
Xft.antialias: 1
Xft.hinting:   1
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.rgba:      rgb
Xft.autohint:  0
